pipeline {
    agent any

    stages {
        stage('git') {
            steps {
               git branch: 'main', url: 'https://github.com/akale9888/akash.git'
            }
        }
          
        stage('mvn build') {
            steps {
               sh'mvn clean package'
            }
        }
         
          stage('deploy') {
            steps {
               echo "hello"
            }
        }
      }
  }
